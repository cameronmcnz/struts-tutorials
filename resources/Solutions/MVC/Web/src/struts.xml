<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd"> 
<struts>

    <constant name="struts.enable.DynamicMethodInvocation" value="false" />
    <constant name="struts.devMode" value="false" />

    <!-- Add packages here -->
    <package name="store" namespace="/store" extends="struts-default">

        <!-- Add actions here -->
        <action name="CheckInstall" class="triveratunes.web.CheckInstall">
            <result>/store/CheckInstall.jsp</result>
        </action>

        <action name="hotCDAction" class="triveratunes.web.HotCDAction">
            <result>/store/hot_cd.jsp</result>
        </action>


    </package>



</struts>
